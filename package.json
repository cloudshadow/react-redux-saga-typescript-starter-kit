{
  "name": "react-redux-saga-typescript-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "webpack serve --hot --color --config ./webpack.config.dev.js",
    "start:prod": "npm run clean-dist && webpack --config ./webpack.config.prod.js --color",
    "start:test": "npm run clean-dist-stable && webpack --config ./webpack.config.test.js",
    "eslint": "./node_modules/.bin/eslint ./src/**/*.{ts,tsx}",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "7.22.5",
    "@deficonnect/types": "2.0.3",
    "@reduxjs/toolkit": "1.9.5",
    "@types/history": "5.0.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/react-redux": "7.1.25",
    "@types/react-router": "5.1.20",
    "axios": "1.4.0",
    "history": "5.3.0",
    "jwt-decode": "3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.0",
    "react-router": "6.13.0",
    "react-router-dom": "6.13.0",
    "redux": "4.2.1",
    "redux-saga": "1.2.3",
    "redux-thunk": "2.4.0",
    "rxjs": "7.4.0",
    "styled-components": "5.3.6",
    "typesafe-actions": "5.1.0",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.22.5",
    "@babel/core": "7.22.5",
    "@babel/node": "7.22.5",
    "@babel/plugin-transform-runtime": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@types/styled-components": "5.1.26",
    "@types/webpack-env": "1.18.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "babel-loader": "9.1.2",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "eslint": "8.43.0",
    "eslint-plugin-react": "7.32.2",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "immer": "10.0.2",
    "jest": "29.5.0",
    "json-loader": "0.5.7",
    "lint-staged": "13.2.2",
    "mini-css-extract-plugin": "2.7.6",
    "postcss-loader": "7.3.3",
    "prettier": "2.8.8",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "thread-loader": "4.0.2",
    "ts-loader": "9.4.3",
    "webpack": "5.87.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
