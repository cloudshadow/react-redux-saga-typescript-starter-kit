{
  "name": "react-redux-saga-typescript-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "webpack serve --hot --color --config ./webpack.config.dev.js",
    "start:prod": "npm run clean-dist && webpack --config ./webpack.config.prod.js --color",
    "start:test": "npm run clean-dist-stable && webpack --config ./webpack.config.test.js",
    "eslint": "./node_modules/.bin/eslint ./src/**/*.{ts,tsx}",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "@deficonnect/types": "^2.0.3",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/history": "5.0.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.8",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/react-redux": "7.1.24",
    "@types/react-router": "5.1.19",
    "axios": "1.1.3",
    "history": "5.3.0",
    "jwt-decode": "3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router": "6.4.3",
    "react-router-dom": "^6.4.3",
    "redux": "4.2.0",
    "redux-saga": "^1.2.1",
    "redux-thunk": "2.4.0",
    "rxjs": "7.4.0",
    "styled-components": "^5.3.6",
    "typesafe-actions": "5.1.0",
    "typescript": "4.5.2"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/node": "7.20.7",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@types/styled-components": "^5.1.26",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "babel-loader": "9.1.2",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "eslint": "8.33.0",
    "eslint-plugin-react": "7.32.2",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "immer": "9.0.19",
    "jest": "29.4.1",
    "json-loader": "0.5.7",
    "lint-staged": "13.1.0",
    "mini-css-extract-plugin": "2.7.2",
    "postcss-loader": "7.0.2",
    "prettier": "2.4.1",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.6",
    "thread-loader": "^3.0.4",
    "ts-loader": "9.4.2",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
